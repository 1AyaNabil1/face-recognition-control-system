{
    "info": {
        "name": "Face Recognition API",
        "description": "Production-grade face recognition system",
        "version": "1.0.0"
    },
    "item": [
        {
            "name": "Authentication",
            "item": [
                {
                    "name": "Login",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/api/v1/auth/login",
                            "host": ["{{base_url}}"],
                            "path": ["api", "v1", "auth", "login"]
                        },
                        "body": {
                            "mode": "raw",
                            "raw": {
                                "username": "admin",
                                "password": "your-password"
                            }
                        }
                    }
                },
                {
                    "name": "Refresh Token",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{refresh_token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/api/v1/auth/refresh",
                            "host": ["{{base_url}}"],
                            "path": ["api", "v1", "auth", "refresh"]
                        }
                    }
                }
            ]
        },
        {
            "name": "Face Recognition",
            "item": [
                {
                    "name": "Register Face",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{access_token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/api/v1/faces/register",
                            "host": ["{{base_url}}"],
                            "path": ["api", "v1", "faces", "register"]
                        },
                        "body": {
                            "mode": "formdata",
                            "formdata": [
                                {
                                    "key": "image",
                                    "type": "file",
                                    "src": []
                                },
                                {
                                    "key": "name",
                                    "type": "text",
                                    "value": "John Doe"
                                },
                                {
                                    "key": "model",
                                    "type": "text",
                                    "value": "ensemble"
                                },
                                {
                                    "key": "overwrite",
                                    "type": "boolean",
                                    "value": false
                                }
                            ]
                        }
                    }
                },
                {
                    "name": "Recognize Face",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{access_token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/api/v1/faces/recognize",
                            "host": ["{{base_url}}"],
                            "path": ["api", "v1", "faces", "recognize"]
                        },
                        "body": {
                            "mode": "formdata",
                            "formdata": [
                                {
                                    "key": "image",
                                    "type": "file",
                                    "src": []
                                },
                                {
                                    "key": "model",
                                    "type": "text",
                                    "value": "ensemble"
                                },
                                {
                                    "key": "min_confidence",
                                    "type": "number",
                                    "value": 0.6
                                },
                                {
                                    "key": "include_attributes",
                                    "type": "boolean",
                                    "value": true
                                },
                                {
                                    "key": "max_faces",
                                    "type": "number",
                                    "value": 5
                                }
                            ]
                        }
                    }
                },
                {
                    "name": "Delete Face",
                    "request": {
                        "method": "DELETE",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{access_token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/api/v1/faces/{name}",
                            "host": ["{{base_url}}"],
                            "path": ["api", "v1", "faces", "{name}"]
                        }
                    }
                },
                {
                    "name": "List Faces",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{access_token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/api/v1/faces",
                            "host": ["{{base_url}}"],
                            "path": ["api", "v1", "faces"],
                            "query": [
                                {
                                    "key": "page",
                                    "value": "1"
                                },
                                {
                                    "key": "per_page",
                                    "value": "10"
                                }
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "System",
            "item": [
                {
                    "name": "Health Check",
                    "request": {
                        "method": "GET",
                        "url": {
                            "raw": "{{base_url}}/health",
                            "host": ["{{base_url}}"],
                            "path": ["health"]
                        }
                    }
                },
                {
                    "name": "Metrics",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{access_token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/metrics",
                            "host": ["{{base_url}}"],
                            "path": ["metrics"]
                        }
                    }
                },
                {
                    "name": "Model Info",
                    "request": {
                        "method": "GET",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{access_token}}"
                            }
                        ],
                        "url": {
                            "raw": "{{base_url}}/api/v1/models/info",
                            "host": ["{{base_url}}"],
                            "path": ["api", "v1", "models", "info"]
                        }
                    }
                }
            ]
        }
    ],
    "variable": [
        {
            "key": "base_url",
            "value": "https://your-domain.modal.run"
        },
        {
            "key": "access_token",
            "value": "your-access-token"
        },
        {
            "key": "refresh_token",
            "value": "your-refresh-token"
        }
    ]
} 